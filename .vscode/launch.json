    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": "--model resnet152 --pretrained_weights pretrained_weights/r3d152_KM_200ep.pth"
            "terminal.integrated.shell.linux": "/usr/bin/tmux",
            "terminal.integrated.shellArgs.linux": [
                "new-session",
                "-A",
                "-s",
                "main"
            ]
        }
    ]
}